[init]
	defaultBranch = main
[color]
	ui = auto
[pull]
	rebase = false
[core]
	editor = code --wait
[includeIf "gitdir:~/**"]
    path = ~/Repos/.gitconfig_corpo
[includeIf "gitdir:~/Repos/Corporate/**"]
    path = ~/Repos/.gitconfig_corpo
[includeIf "gitdir:~/Repos/Personal/**"]
    path = ~/Repos/.gitconfig_perso
