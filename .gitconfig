[init]
	defaultBranch = main
[color]
	ui = auto
[pull]
	rebase = false
[core]
	editor = code --wait
[includeIf "gitdir:~/**"]
    path = ~/GitCorpo/.gitconfig_corpo
[includeIf "gitdir:~/GitCorpo/**"]
    path = ~/GitCorpo/.gitconfig_corpo
[includeIf "gitdir:~/GitPerso/**"]
    path = ~/GitPerso/.gitconfig_perso
